<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:l="urn:import:com.medassets.report.client.library">

	<ui:style src="../resources/GlobalStyles.css" />

	<g:DockLayoutPanel unit='EM' styleName="{style.westPanel}">
		<g:north size="5">
			<g:HTMLPanel styleName="{style.treeToolbarPanel}">
				<g:HorizontalPanel  styleName="{style.treeButtonPanel}">
                    <g:Label ui:field="expandAll" styleName="{style.treeLinks}">Expand All</g:Label>
                    <g:Label ui:field="collapseAll" styleName="{style.treeLinks}">Collapse All</g:Label>
				</g:HorizontalPanel>
				<g:FlowPanel ui:field="buttonPanel" styleName="{style.treeToolbarButtonPanel}" />
			</g:HTMLPanel>
		</g:north>
		<g:center>
			<g:ScrollPanel ui:field="scrollPanel">
			</g:ScrollPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
