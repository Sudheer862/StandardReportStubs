<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:app="urn:import:com.medassets.report.client.library"
	xmlns:common="urn:import:com.medassets.report.client.common">

	<ui:style src="../resources/GlobalStyles.css" />

    <g:DockLayoutPanel unit='EM'>
        <g:north size="4.5">
            <g:VerticalPanel width="100%">
                <common:Header />
                <g:Label styleName="{style.pageHeader}">Standard, Flexible, and OLAP Report
                    Library</g:Label>
            </g:VerticalPanel>
        </g:north>
        <g:center>


             <app:OneWidgetLayoutPanel ui:field="mainViewPanel"/>

        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>