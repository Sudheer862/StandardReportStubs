<?xml version="1.0" encoding="UTF-8"?>

        <!--

        ############################### IMPORTANT! ###############################

        This file is NOT used. Consider it an example. To add a new service/servlet edit the web.xml in Alliance Web src/webapps/WEB-INF

        -->

<web-app version="2.4"
        xmlns="http://java.sun.com/xml/ns/j2ee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
            http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

    <servlet>
        <servlet-name>mainService</servlet-name>
        <servlet-class>com.medassets.report.server.MainServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mainService</servlet-name>
        <url-pattern>/main/mainService</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>flexService</servlet-name>
        <servlet-class>com.medassets.report.server.FlexServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>flexService</servlet-name>
        <url-pattern>/main/flexService</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>standardService</servlet-name>
        <servlet-class>com.medassets.report.server.StandardServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>standardService</servlet-name>
        <url-pattern>/main/standardService</url-pattern>
    </servlet-mapping>
	<servlet>
        <servlet-name>flexServiceDev</servlet-name>
        <servlet-class>com.medassets.report.server.FlexServiceImpl</servlet-class>
        <init-param>
			<param-name>USE_MOCK_DATA</param-name>
			<param-value>true</param-value> <!-- Must be boolean -->
		</init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>flexServiceDev</servlet-name>
        <url-pattern>/flexselectionstubs/flexService</url-pattern>
    </servlet-mapping>
</web-app>
