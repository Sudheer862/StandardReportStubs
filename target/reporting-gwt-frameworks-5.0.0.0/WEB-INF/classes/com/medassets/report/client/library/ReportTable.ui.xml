<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:l="urn:import:com.medassets.report.client.library" xmlns:s="urn:import:com.medassets.report.client.standard">

	<ui:style src="../resources/GlobalStyles.css" />

	<g:DockLayoutPanel unit='EM'>
		<g:north size="3.1">
			<g:HTMLPanel styleName="{style.toolbarPanel}">
				<g:FlowPanel ui:field="buttonPanel" styleName="{style.toolbarButtonPanel}"/>
				<g:TextBox ui:field="searchField" styleName="{style.searchText}" />
			</g:HTMLPanel>
		</g:north>
		<g:south size="2.3">
			<g:HTMLPanel ui:field="pagerPanel" styleName="{style.pagerPanel}" />
		</g:south>
		<g:center>
			<g:ScrollPanel ui:field="scrollPanel" styleName="{style.bottomBorder}">
			</g:ScrollPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
