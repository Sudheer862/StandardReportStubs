<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:flex="urn:import:com.medassets.report.client.flex"
	xmlns:m="urn:import:com.medassets.report.client">

	<g:TabLayoutPanel barHeight="2.5" barUnit="EM">
		<g:tab>
			<g:header>General</g:header>
			<g:ScrollPanel>
				<flex:FlexGeneralPanel ui:field="flexGeneralPanel" />
			</g:ScrollPanel>
		</g:tab>
		<g:tab>
			<g:header>Selections</g:header>
			<flex:FlexSelectPanel ui:field="flexSelectPanel" />
		</g:tab>
		<g:tab>
			<g:header>Sorts</g:header>
			<g:HTMLPanel></g:HTMLPanel>
		</g:tab>
		<g:tab>
			<g:header>Global Settings</g:header>
			<g:ScrollPanel>
				<flex:FlexGlobalSettingsPanel ui:field="flexGlobalPanel" />
			</g:ScrollPanel>
		</g:tab>
	</g:TabLayoutPanel>


</ui:UiBinder>
