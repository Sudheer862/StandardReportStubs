<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:flex="urn:import:com.medassets.report.client.flex">

    <ui:style src="../resources/GlobalStyles.css"/>

    <g:HTMLPanel>
        <g:VerticalPanel>
            <g:HorizontalPanel styleName="{style.dialogButtonPanel2}" spacing="5">
                <g:Button text="Save" ui:field="saveButton"></g:Button>
                <g:Button text="Save As" ui:field="saveAsButton"></g:Button>
                <g:Button text="Run" ui:field="runButton"></g:Button>
            </g:HorizontalPanel>
            <flex:FlexReport ui:field="flexReport" width="600px" height="500px"></flex:FlexReport>
        </g:VerticalPanel>
    </g:HTMLPanel>

</ui:UiBinder>
